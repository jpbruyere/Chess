Button {
	Width = 81;
	Height = 50;
	Background = vgradient|0:DimGray|1:Black;
	Font = droid bold, 14;
}
Window {
	Template = #Chess.gui.tmpWindow.crow;
}
Menu {
	Background = vgradient|0:0.6,0.6,0.6,0.9|1:0.4,0.4,0.4,0.6;
}
ColorPicker {
	Background = vgradient|0:0.6,0.6,0.6,0.9|1:0.4,0.4,0.4,0.6;
}
MenuItem {
	Margin=2;
	Font = droid bold, 14;

}
Spinner {
	Width=60;
	Font = droid bold, 14;
}
FpsLabel {
	Width = 50%;
	Font = droid, 10;
	Margin = 1;
	TextAlignment = Center;
	Background = Jet;
}
FpsDisp {
	Font = droid , 12;
	Width = 50%;
	TextAlignment = Center;
	Background = Teal;
}
VSInMenu {
	Width=Fit;
	Height=Fit;
	Background = Jet;
	CornerRadius = 10;
}
LabInMenu {
	Margin = 5;
	Font = droid, 10;
	Background = DimGray;
	Foreground = Ivory;
}
ColorSelector {
	Template=#Chess.gui.ColorSelector.crow;
	Margin=2;
	Background = vgradient|0:DimGray|1:Black;
	Font=droid bold, 14;
	MouseEnter={Background=vgradient|0:Gray|1:Onyx;};
	MouseLeave={Background=vgradient|0:DimGray|1:Black};
	Width=90%;
	Height=Fit;
}
BigCheckBox {
	Template=#Chess.gui.tmpCheckBox.crow;
	Margin=2;
	Foreground=Black;
	Font=droid bold, 14;
	MouseEnter={Background=DimGray};
	MouseLeave={Background=Transparent};
	Checked={Foreground=White};
	Unchecked={Foreground=Black};
}
BigSpinner {
	Template=#Chess.gui.tmpSpinner.crow;
	Width=Fit;
	Height=Fit;
}